.les-evaluate-close-panel i{
  color: #ffffff;
  font-size: 50rpx;
}

.les-evaluate-content-inner{
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  width: 100%;
  height: 0;
  flex: 1 1 auto;
  z-index: 100;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
  background-color: #fff;
  padding: 20rpx;
}

.les-evaluate-line-box{
  width: 100%;
  flex: 1 1 auto;
  z-index: 100;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  color: #a19f9f;
  padding-left: 10rpx;
}

.les-evaluate-line-box text{
  font-size:36rpx;
  margin-left: 10rpx;
  margin-top: 12rpx;
}

.les-evaluate-line-box i{
  font-size: 46rpx;
  margin-left: 20rpx;
}

.les-evaluate-line-box i.ative{
  color: #F14141;
}

.les-evaluate-submit-box{
  width: 100%;
  height:90rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  background-color: #F14141;
  align-items: center;
  color:#fff;
  margin-top: 30rpx;
}

.les-evaluate-submit-box text{
  font-size: 30rpx;
}

.les-none-les{
  position: fixed;
  top:0rpx;
  left: 0rpx;
  height: 100%;
  width: 100%;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background-color: #ffffff;
  display: none;
}

.les-none-les.show{
  display: flex;
}

.les-none-les-box{
  width: 100%;
  height: 100rpx;
  font-size: 50rpx;
  color: #777;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  z-index: 10000;
}
