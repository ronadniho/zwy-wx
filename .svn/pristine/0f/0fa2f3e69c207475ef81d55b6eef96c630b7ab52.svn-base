}

.les-list {
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding: 0rpx;
}

.les-list scroll-view {
  width: 100%;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.les-list-item {
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding: 0rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  position: relative;
  margin-top: 15rpx;
  overflow-x: scroll;
  background-color: #fff;
  height: 230rpx;
  overflow-y: hidden;
}

.les-list-item-header {
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding: 0rpx;
  width: 250rpx;
  height: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  position: relative;
  overflow: hidden;
}

.les-list-item-header .image-box {
  width: 230rpx;
  height: 200rpx;
  position: relative;
  overflow: hidden;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  border-radius: 7rpx;
}

.les-list-item-header image {
  width: 230rpx;
  height: 200rpx;
}

.image-ling {
  position: absolute;
  left: 50%;
  top: 50%;
  margin: -30rpx 0 0 -30rpx;
  width: 60rpx !important;
  height: 60rpx !important;
}

.corner-box {
  position: absolute;
  right: -53rpx;
  top: -53rpx;
  width: 100rpx;
  height: 100rpx;
  background-color: #f14141;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: flex-end;
  padding-bottom: 2rpx;
  overflow: hidden;
  color: #fff;
  transform: rotate(45deg);
}

.corner-box text {
  transform: rotate(-45deg);
  font-size: 26rpx;
}

.time-shade {
  position: absolute;
  left: 12rpx;
  top: 17rpx;
  width: 100%;
  height: 100%;
  z-index: 10;
  display: none;
  flex-direction: row;
  align-items: flex-start;
  justify-content: flex-start;
  border-radius: 7prx 0rpx 0rpx 0rpx;
}

.time-shade view {
  width: 130rpx;
  height: 35rpx;
  background-color: #777;
  opacity: 0.7;
}

.time-shade.show {
  display: flex;
}

.time-panel {
  position: absolute;
  left: 15rpx;
  top: 17rpx;
  color: #fff;
  font-size: 23rpx;
  display: none;
  z-index: 11;
}

.time-panel i {
  font-size: 23rpx;
  margin-right: 4rpx;
}

.time-panel.show {
  display: block;
}

.les-list-item-content {
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding: 0rpx;
  width: 0;
  height: 100%;
  flex: 1 1 auto;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
  padding-right: 20rpx;
  padding-top: 14rpx;
  padding-bottom: 15rpx;
}

.les-list-item-content-title {
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding: 0rpx;
  display: block;
  width: 100%;
  flex: 1 1 auto;
}

.les-list-item-content-title-content {
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding: 0rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  font-size: 30rpx;
  color: #626262;
}

.les-list-item-content-title::before {
  content: '';
  display: block;
  clear: both;
}

.les-list-item-content-title::after {
  content: '';
  display: block;
  clear: both;
}

.les-list-item-content-title-content.currentPlay {
  color: #f14143;
}

.les-list-item-content-tearcher-info {
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding: 0rpx;
  display: block;
  height: 50rpx;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 30rpx;
  color: #a8a8a8;
  margin-bottom: 1rpx;
}

.les-list-item-content-status-box {
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding: 0rpx;
  display: flex;
  width: 100%;
  height: 90rpx;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
}

.les-list-item-content-process-box {
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding: 0rpx;
  display: flex;
  width: 100%;
  height: 30rpx;
  position: relative;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}

.les-list-item-content-process {
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding: 0rpx;
  display: flex;
  width: 0;
  flex: 1 1 auto;
  height: 15rpx;
  position: relative;
  background-color: #ebebeb;
  flex-direction: row;
  justify-content: flex-start;
  align-items: flex-start;
  border-radius: 10rpx;
}

.les-list-item-content-process-line {
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding: 0rpx;
  position: absolute;
  display: flex;
  width: 0%;
  height: 100%;
  background-color: #fea116;
  border-radius: 10rpx;
}

.les-list-item-content-process-text {
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding: 0rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  width: 90rpx;
  height: 100%;
  color: #898888;
  font-size: 24rpx;
}

.les-list-item-content-process-text.color3 {
  color: #fea116;
}

.les-list-item-content-process-line.complete {
  background-color: #5bb85d;
}

.les-list-item-content-process-text.complete {
  color: #5bb85d;
}

.les-list-item-content-icons {
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding: 0rpx;
  display: flex;
  width: 100%;
  height: 0;
  flex: 1 1 auto;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.les-list-item-content-icons-item {
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding: 0rpx;
  display: flex;
  width: 130rpx;
  height: 100%;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}

.les-list-item-content-icons-item i {
  font-size: 35rpx;
  color: #a8a8a8;
}

.les-list-item-content-icons-item i.bigstar {
  font-size: 40rpx;
}

.les-list-item-content-icons-item text {
  font-size: 30rpx;
  color: #a9a9a9;
}

.les-item-del-panel {
  width: 120rpx;
  height: 100%;
  background-color: #f34141;
  position: absolute;
  right: -120rpx;
  top: 0rpx;
  display: none;
