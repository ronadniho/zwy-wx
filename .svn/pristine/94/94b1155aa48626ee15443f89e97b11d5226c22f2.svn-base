/* pages/live/special.wxss */
page{
  background-color: #f2f2f2;
}
.live-title{
  padding: 20rpx;
  text-align: left;
  width: 100%;
  box-sizing: border-box;
}

.item-container{
  width: 100%;
  box-sizing: border-box;
}

.iitem-block{
  display: block;
  width: 100%;
  box-sizing: border-box;
  padding: 20rpx;
  background-color: #fff;
  margin-bottom: 20rpx;
  box-shadow: 0rpx 5rpx 5rpx rgba(0, 0, 0, 0.05);
}
.iitem-block image{
  width: 100%;
  height: 430rpx;
}
.subcolor{
  color: #999;
}
.font30{
  font-size: 30rpx;
}
.mb20{
  margin-bottom: 20rpx;
}
.mt20{
  margin-top: 20rpx;
}