  border: none;
  margin: 0rpx;
  padding: 0rpx;
}

.les-list scroll-view {
  width: 100%;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.les-list-item {
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding: 0rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  position: relative;
  margin-top: 15rpx;
  overflow-x: scroll;
  background-color: #fff;
  height: 230rpx;
  overflow-y: hidden;
}

.les-list-item-header {
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding: 0rpx;
  width: 250rpx;
  height: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  position: relative;
  overflow: hidden;
}

.les-list-item-header .image-box {
  width: 230rpx;
  height: 200rpx;
  position: relative;
  overflow: hidden;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center; 
  border-radius: 7rpx;
  text-align: center;
}

.les-list-item-header image {
  width: 230rpx;
  height: 200rpx;
}

.image-ling {
  position: absolute;
  left: 50%;
  top: 50%;
  margin: -30rpx 0 0 -30rpx;
  width: 60rpx !important;
  height: 60rpx !important;
}

.corner-box {
  position: absolute;
  right: -53rpx;
  top: -53rpx;
  width: 100rpx;
  height: 100rpx;
  background-color: #f14141;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: flex-end;
  padding-bottom: 2rpx;
  overflow: hidden;
  color: #fff;
  transform: rotate(45deg);
}

.corner-box text {
  transform: rotate(-45deg);
  font-size: 26rpx;
}

.time-shade {
  position: absolute;
  left: 12rpx;
  top: 17rpx;
  width: 100%;
  height: 100%;
  z-index: 10;
  display: none;
  flex-direction: row;
  align-items: flex-start;
  justify-content: flex-start;
  border-radius: 7prx 0rpx 0rpx 0rpx;
}

.time-shade view {
  width: 130rpx;
  height: 35rpx;
  background-color: #777;
  opacity: 0.7;
}

.time-shade.show {
  display: flex;
}

.time-panel {
  position: absolute;
  left: 15rpx;
  top: 17rpx;
  color: #fff;
  font-size: 23rpx;
  display: none;
  z-index: 11;
}

.time-panel i {
  font-size: 23rpx;
  margin-right: 4rpx;
}

.time-panel.show {
  display: block;
}

