}

.container .header .header-search .input {
  flex: 5;
  background: #fff;
  margin-top: -2rpx;
  margin-right: 5rpx;
  margin-left: 20rpx;
}

.container .header .header-search .search-img {
  flex: 1;
  text-align: center;
  padding-top: 17rpx;
  color: #ffb0b3;
}

.container .header .text {
  margin: 6rpx 0;
}

.container .header text {
  font-size: 22rpx;
  color: #a8a8a8;
}

/*内容栏  */

.container .box {
 width: 100%;
