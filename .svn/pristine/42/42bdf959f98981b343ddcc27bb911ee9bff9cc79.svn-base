/*司局级必修专题  */
.sjjbxzt-container,Teacher-container{
  width: 100%;
  padding: 0rpx;
  background-color: #fff;
  margin-top: 20rpx;
  box-shadow: 0rpx 5rpx 5rpx rgba(0, 0, 0, 0.05);
}
.sjjbxzt-title,Teacher-title{
  position: relative;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: left;
  box-sizing: border-box;
  padding:0rpx 30rpx;
  font-size: 36rpx;
  border-bottom: 1px solid #dcdcdc;
}
.active-color{
  color: #F34141;
}
.item-container{
  width: 100%;
  box-sizing: border-box;
  padding:0rpx 10rpx 20rpx 10rpx;
}
.iitem{
  box-sizing: border-box;
  float: left;
  width: 50%;
  padding: 10rpx;
  font-size: 26rpx;
}
.iitem image{
  width: 100%;
  height: 230rpx;
}
.subcolor{
