  margin: 0 10rpx;
}

/*课程  */

.tabcontent-container{
  width: 100%;
}
.tabcontent-container .content-item{
  width: 100%;
  box-sizing: border-box;
  padding: 20rpx 20rpx 20rpx 340rpx;
  background-color: #fff;
  margin-top: 20rpx;
  box-shadow: 0rpx 5rpx 5rpx rgba(0, 0, 0, 0.05);
  position: relative;
  height: 260rpx;
  overflow: hidden;
  font-size: 36rpx;
}
.content-item image{
  position: absolute;
  left: 20rpx;
  top: 20rpx;
  width: 300rpx;
  height: 220rpx;
}
.content-item .item-title{
  height: 120rpx;
  line-height: 40rpx;
  margin-bottom: 10rpx;
  box-sizing: border-box;
  margin-top: 5rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
}
.content-item .zuozhe{
  display: flex;
}
.content-item .item-zjr{
  flex: 1;
  height: 70rpx;
  margin-bottom: 10rpx;
  line-height: 36rpx;
  overflow: hidden;
  box-sizing: border-box;
}
.content-item .item-time{
  position: absolute;
  left: 21rpx;
  top: 21rpx;
  color: #fff;
  font-size: 30rpx;
  background-color: rgba(0, 0, 0, 0.2);
  z-index: 9;
  padding: 5rpx 10rpx;
  border-top-left-radius: 10rpx;
}
.content-item .item-time i{
  margin-right: 10rpx;
}
.tab-container{
  width: 100%;
  box-sizing: border-box;
  padding: 0rpx 130rpx 0rpx 20rpx;
  background-color: #fff;
  margin-top: 20rpx;
  box-shadow: 0rpx 5rpx 5rpx rgba(0, 0, 0, 0.05);
  position: relative;
  font-size: 36rpx;
}
.tab-item{
  display: inline-block;
  height: 90rpx;
  box-sizing: border-box;
  margin-right: 30rpx;
  padding: 20rpx 0rpx;
}
.tab-item.active{
  color: #F34141;
  border-bottom: 2px solid #F34141;
}
.tab-container scroll-view{
  height: 90rpx;
  width: 100%;
}
.more-tab{
  position: absolute;
  right: 20rpx;
  top: 0rpx;
  height: 90rpx;
  line-height: 90rpx;
}
.tablist{
  white-space: nowrap;
}
.tabcontent-container .hover-class{
  background-color: #f2f2f2;
}
