page {
  height: 100%;
  overflow: hidden;
}

.les-box {
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding: 0rpx;
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
}

.les-scroll{
  width: 100%;
  height: 0;
  flex: 1 1 auto;
}

.les-video-box {
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  width: 100%;
  height: 360rpx;
  padding: 0rpx;
}

.les-video-box video {
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding: 0rpx;
  width: 100%;
  height: 100%;
  display: none;
}

.les-video-box .listen {
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding: 0rpx;
  width: 100%;
  height: 100%;
  position: relative;
}

.listen-panel {
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding: 0rpx;
  width: 100%;
  height: 80rpx;
  position: absolute;
  bottom: 0rpx;
  opacity: 0;
}

.listen-panel-bg {
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding: 0rpx;
  width: 100%;
  height: 100%;
  position: absolute;
  background-color: #898888;
  opacity: 0.7;
}

.listen-panel-content {
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding: 0rpx;
  width: 100%;
  height: 100%;
  z-index: 10;
  position: absolute;
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  justify-content: flex-start;
}

.listen-panel-play {
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding: 0rpx;
  width: 60rpx;
  height: 100%;
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  justify-content: center;
  color: #fff;
  font-size: 36rpx;
}

.listen-panel-play.play i {
  display: none;
}

.listen-panel-play i.pause-play {
  display: none;
}

.listen-panel-play.play i.pause-play {
  display: inline;
}

.listen-panel-start-end {
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding: 0rpx;
  width: 100rpx;
  height: 100%;
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  justify-content: center;
  color: #fff;
  font-size: 24rpx;
}

.listen-panel-process {
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  width: 0;
  overflow: hidden;
  flex: 1 1 auto;
  height: 100%;
  font-size: 36rpx;
  padding-top: 8rpx;
}

.listen-panel-process slider {
  z-index: 14;
}

.les-video-box.see video {
  display: block;
}

.les-video-box.see .listen {
  display: none;
}

.les-video-box .listen image {
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding: 0rpx;
  width: 100%;
  height: 100%;
}

/* 标题开始 */

