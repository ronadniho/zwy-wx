.les-evaluate-submit-box{
  width: 100%;
  height:90rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  background-color: #F14141;
  align-items: center;
  color:#fff;
  margin-top: 30rpx;
}

.les-evaluate-submit-box text{
  font-size: 30rpx;
}

.les-Anthology{
  width: 100%;
  min-height: 60rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  color: #777;
  font-size: 30rpx;
  padding-left: 20rpx;
}

.les-Anthology.active{
  color: #f04040;
}
