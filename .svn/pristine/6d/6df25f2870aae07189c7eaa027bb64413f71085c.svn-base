
page {
  height: 100%;
}
.container .AssociativeSearch .classsifica-box{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding-top: 20rpx;
  padding: 0rpx 40rpx;
  background: #ffffff;
  z-index: 100;
}

.container .AssociativeSearch .cla-box{
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  border-bottom: 1px solid #dcdada;
  margin-top: 20rpx;
  padding-bottom: 30rpx;
}

.container .AssociativeSearch .cla-box.last{
  border-bottom: none;
  display: none;
}

.container .AssociativeSearch .cla-box.show{
  display: block;
}

.container .AssociativeSearch .cla-box.firline{
  border-bottom: none;
}

.container .AssociativeSearch .cla-box-title{
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding: 0rpx 40rpx;
  color: #7d7a7a;
  font-size: 36rpx;
}

.container .AssociativeSearch .cla-box-content{
  box-sizing: border-box;
  border: none;
  margin: 0rpx;
  padding: 0rpx 40rpx;
  width: 100%;
  color: #7d7a7a;
}

.container .AssociativeSearch .cla-box-content .sing-item{
  padding: 20rpx 20rpx;
  font-size: 30rpx;
  display: inline-block;
  border-radius: 5rpx;
  border: 1px solid #dcdada;
  margin-right: 20rpx;
  margin-top: 20rpx;
}


.container .AssociativeSearch .cla-box-content .sing-item.active{
  background-color: #F14141;
  border: 1px solid #F14141;
  color: #ffffff;
}
